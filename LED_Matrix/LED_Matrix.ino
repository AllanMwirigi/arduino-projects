void displayChar(int arrCol[6][6]){
  for(int i = 0; i < 6; i++){
    setRow(arrRows[i], arrCol[i]);
  }
  //delay(1000);
}

void setRow(int arrRow[], int arrCol[]){
  for(int i = 0; i < 6; i++){
    digitalWrite(ROWS[i], arrRow[i]);
   }
   for(int i = 0; i < 6; i++){
    digitalWrite(COLS[i], arrCol[i]);
   }
   delay(1);
   //remove mirroring
   int arrReset[] = {0,0,0,0,0,0};
   for(int i = 0; i < 6; i++){
    digitalWrite(COLS[i], arrReset[i]);
   }
}